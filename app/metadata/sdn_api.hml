          description: SdnResponse
          fields:
            count:
              description: Count
              type:
                name: Float
                type: named
            results:
              description: Results
              type:
                element_type:
                  name: SdnItem
                  type: named
                type: array
        getGetsdnGetsdnGetsdnGet_query:
          fields:
            name:
