      read:
        valueFromEnv: APP_SDN_API_READ_URL
      write:
        valueFromEnv: APP_SDN_API_WRITE_URL
  headers:
    Authorization:
      valueFromEnv: APP_SDN_API_AUTHORIZATION_HEADER
